<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submit_package</servlet-name>
        <servlet-class>spackage.SubmitPackage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetrievePackage</servlet-name>
        <servlet-class>spackage.RetrievePackage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitAddMember</servlet-name>
        <servlet-class>smembership.SubmitAddMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RetrieveMemberList</servlet-name>
        <servlet-class>smembership.RetrieveMemberList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>dashboard.Dashboard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submit_package</servlet-name>
        <url-pattern>/submit_package</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetrievePackage</servlet-name>
        <url-pattern>/RetrievePackage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitAddMember</servlet-name>
        <url-pattern>/SubmitAddMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RetrieveMemberList</servlet-name>
        <url-pattern>/RetrieveMemberList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
</web-app>
